
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  extensions = [pgvector(map: "vector")]
}

model Product {
  id          String   @id @default(cuid())
  productName String
  description Unsupported("vector(1536)")
  createdAt   DateTime @default(now())
}

model File {
  id       String   @id @default(cuid())
  fileName  String
  createdAt DateTime @default(now())
}

model Category {
  id       String   @id @default(cuid())
  name     String   @unique
  createdAt DateTime @default(now())
}