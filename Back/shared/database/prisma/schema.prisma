
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  extensions = [pgvector(map: "vector")]
}

model Product {
  id          String   @id 
  productName String
  description String
  price       String
  category    String
  embedding   Unsupported("vector(768)")?
  createdAt   DateTime @default(now())
}
