
generator client {
  provider = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  extensions = [pgvector(map: "vector")]
}

model Product {
  id          String   @id @default(cuid())
  productName String
  description String
  embedding   Unsupported("vector")? 
  createdAt   DateTime @default(now())
}
